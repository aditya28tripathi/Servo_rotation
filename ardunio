#include<Servo.h>
Servo myservo;

 void setup() {
myservo.attach(10);  // put your setup code here, to run once:

}

void loop() {
  // put your main code here, to run repeatedly:
for(int pos = 0; pos<=180;pos++){

  myservo.write(pos);
  delay(30);

}

for(int pos = 180;pos<=0;pos++){
  myservo.write(pos);
  delay(30);
}
}
//this is a servo motor rotation for 180 degree
 
